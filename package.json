{
    "name": "backend-exercise-nnamdi16",
    "version": "1.0.0",
    "description": "Backend exercise",
    "main": "dist/index.js",
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "test": "jest --coverage --detectOpenHandles",
        "test:watch": "jest --coverage --watchAll",
        "test:badges": "npm run test:coverage && jest-coverage-badges",
        "test:coverage": "npm test -- --coverage",
        "build": "rimraf dist && tsc",
        "preserve": "npm run build",
        "serve": "nodemon  dist/index.js",
        "prestart": "npm run build",
        "start": "ts-node ./src/index.ts",
        "lint": "eslint . --ext .ts",
        "lint-and-fix": "eslint . --ext .ts --fix",
        "view:coverage": "serve coverage/lcov-report",
        "clean": "rm -rf coverage src/**/*.js src/**/*.map"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/entyre-hire/backend-exercise-nnamdi16.git"
    },
    "author": "Nwabuokei Nnamdi",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/entyre-hire/backend-exercise-nnamdi16/issues"
    },
    "homepage": "https://github.com/entyre-hire/backend-exercise-nnamdi16#readme",
    "dependencies": {
        "@types/axios": "^0.14.0",
        "axios": "^0.24.0",
        "cross-env": "^7.0.3",
        "jest-coverage-badges": "^1.1.2",
        "node-fetch": "^3.1.0"
    },
    "devDependencies": {
        "@babel/preset-typescript": "^7.16.0",
        "@types/jest": "^27.0.2",
        "@types/node": "^16.11.7",
        "@typescript-eslint/eslint-plugin": "^5.3.1",
        "@typescript-eslint/parser": "^5.3.1",
        "eslint": "^8.2.0",
        "jest": "^27.3.1",
        "nodemon": "^2.0.15",
        "rimraf": "^3.0.2",
        "supertest": "^6.1.6",
        "ts-jest": "^27.0.7",
        "ts-node": "^10.4.0",
        "typescript": "^4.4.4"
    }
}